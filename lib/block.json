{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "valve/custom-stories-query",
	"version": "0.1.0",
	"title": "Custom Stories Query",
	"category": "valve-blocks",
	"icon": "",
	"description": "The block to display custom posts from fit-story post type.",
  "supports": {
    "align": ["wide", "full"],
    "html": false
  },
  "attributes": {
    "blockId": {
      "type": "string"
    },
    "innerBlocksType": {
      "type": "string",
      "default": "core/query"
    },
    "postId": {
      "type": "string",
      "default": ""
    },
    "selectedPosts": {
      "type": "array",
      "default": [],
      "items": {
        "type": "string"
      }
    },
    "postsPerPage": {
      "type": "number",
      "default": 6
    },
    "showExcerpt": {
      "type": "boolean",
      "default": true
    },
    "showFeaturedImage": {
      "type": "boolean",
      "default": true
    }
  },
	"example": {},
	"textdomain": "fitech-theme",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php",
	"viewScript": "file:./view.js"
}
